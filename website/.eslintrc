{
  "extends": [
    "wesbos"
  ],
  "rules": {
    "no-console": 0,
    "no-plusplus": 0,
    "prefer-destructuring": 0,
    "import/no-unresolved": 0,
    "import/no-extraneous-dependencies": 0,
    "react/prop-types": 0,
    "react/destructuring-assignment": 0,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 120,
        "tabWidth": 2,
        "semi": false
      }
    ]
  }
}